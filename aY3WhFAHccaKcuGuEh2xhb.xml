        </aY3WhFAHccaKcuGuEh2xhb>
      </instance>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/aY3WhFAHccaKcuGuEh2xhb/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/aY3WhFAHccaKcuGuEh2xhb/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="deviceid" nodeset="/aY3WhFAHccaKcuGuEh2xhb/deviceid" type="string"/>
      <bind jr:preload="property" jr:preloadParams="phonenumber" nodeset="/aY3WhFAHccaKcuGuEh2xhb/phonenumber" type="string"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/User_ID" required="true()" type="string"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/Name" required="true()" type="string"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/Gender" required="true()" type="string"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/Date_of_Birth" required="false()" type="date"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/Email_Address" required="true()" type="string"/>
      <bind jr:constraintMsg="Must be 11 digits (Nigeria format)" nodeset="/aY3WhFAHccaKcuGuEh2xhb/Phone_Number" required="true()" type="int"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/Local_Government" required="true()" type="string"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/Type_of_Disability" required="true()" type="string"/>
      <bind nodeset="/aY3WhFAHccaKcuGuEh2xhb/House_Address" required="false()" type="string"/>
      <bind jr:preload="uid" nodeset="/aY3WhFAHccaKcuGuEh2xhb/meta/instanceID" readonly="true()" type="string"/>
